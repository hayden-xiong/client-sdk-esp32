# Example configuration for using dynamic token from API
# Copy this to sdkconfig.defaults and modify as needed

CONFIG_IDF_TARGET="esp32s3"
CONFIG_LK_EXAMPLE_CODEC_BOARD_TYPE="S3_Korvo_V2"

# Use dynamic token from API instead of sandbox
CONFIG_LK_EXAMPLE_USE_DYNAMIC_TOKEN=y
CONFIG_LK_EXAMPLE_API_URL="https://cloud-api.livekit.io/api/sandbox/connection-details"
CONFIG_LK_EXAMPLE_SANDBOX_ID_HEADER="live-agent-demo-26u7lg"
CONFIG_LK_EXAMPLE_DYNAMIC_ROOM_NAME="demo-room"
CONFIG_LK_EXAMPLE_DYNAMIC_PARTICIPANT_NAME="demo-user"
CONFIG_LK_EXAMPLE_AGENT_NAME="hospital_assistant"
CONFIG_LK_EXAMPLE_AGENT_METADATA="role=hospital_assistant;source=triage"

CONFIG_LK_EXAMPLE_SPEAKER_VOLUME=85

CONFIG_BSP_I2C_NUM=0
CONFIG_CODEC_I2C_BACKWARD_COMPATIBLE=n
CONFIG_COMPILER_OPTIMIZATION_PERF=y
CONFIG_ESP_DEFAULT_CPU_FREQ_MHZ_240=y
CONFIG_ESP_WS_CLIENT_ENABLE_DYNAMIC_BUFFER=y
CONFIG_ESP_WS_CLIENT_SEPARATE_TX_LOCK=y
CONFIG_ESP32S3_DATA_CACHE_64KB=y
CONFIG_ESP32S3_DATA_CACHE_LINE_64B=y
CONFIG_ESP32S3_INSTRUCTION_CACHE_32KB=y
CONFIG_ESPTOOLPY_FLASHFREQ_120M=y
CONFIG_ESPTOOLPY_FLASHMODE_QIO=y
CONFIG_ESPTOOLPY_FLASHSIZE_4MB=y
CONFIG_FREERTOS_HZ=1000
CONFIG_FREERTOS_TIMER_TASK_STACK_DEPTH=4096
CONFIG_IDF_EXPERIMENTAL_FEATURES=y
CONFIG_LWIP_SNTP_MAX_SERVERS=2
CONFIG_MBEDTLS_EXTERNAL_MEM_ALLOC=y
CONFIG_MBEDTLS_SSL_DTLS_SRTP=y
CONFIG_MBEDTLS_SSL_PROTO_DTLS=y
CONFIG_PARTITION_TABLE_CUSTOM=y
CONFIG_SPIRAM_FETCH_INSTRUCTIONS=y
CONFIG_SPIRAM_MALLOC_ALWAYSINTERNAL=256
CONFIG_SPIRAM_MALLOC_RESERVE_INTERNAL=8192
CONFIG_SPIRAM_MODE_OCT=y
CONFIG_SPIRAM_SPEED_120M=y
CONFIG_SPIRAM_TRY_ALLOCATE_WIFI_LWIP=y
CONFIG_SPIRAM=y